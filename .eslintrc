{
  /* See all the pre-defined configs here: https://www.npmjs.com/package/eslint-config-defaults */
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 11
  },
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "eqeqeq": 2,
    "comma-dangle": 1,
    "no-console": 0,
    "no-debugger": 1,
    "no-extra-semi": 1,
    "no-irregular-whitespace": 1,
    "no-undef": 1,
    "no-unused-vars": 1,
    "semi": 1,
    "semi-spacing": 1,
    "quotes": [2, "single", { "avoidEscape": true }]
  }
}
